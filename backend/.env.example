# Server Configuration
NODE_ENV=development
PORT=5000

# Database
MONGODB_URI=mongodb://localhost:27017/neighbornet
# Or use MongoDB Atlas:
# MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/neighbornet?retryWrites=true&w=majority

# CORS
CLIENT_URL=http://localhost:3000

# Gemini AI
GEMINI_API_KEY=your_gemini_api_key_here

# AI Configuration
AI_CACHE_TTL_MINUTES=60
AI_CACHE_MAX_SIZE=1000
AI_RATE_LIMIT_PER_MINUTE=30

# Cloudinary (for image uploads)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# OpenAI API (for AI message suggestions)
OPENAI_API_KEY=your_openai_api_key

# Google Maps API (optional - for geocoding)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
